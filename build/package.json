{
  "name": "formkiq-client-sdk-javascript",
  "description": "FormKiQ Client SDK for JavaScript",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "FormKiQ Inc.",
    "email": "info@formkiq.com",
    "url": "https://formkiq.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://formkiq/formkiq-client-sdk-javascript/"
  },
  "bugs": {
    "url": "https://github.com/formkiq/formkiq-client-sdk-javascript/issues"
  },
  "scripts": {
    "babel": "babel --presets es2015 js/main.js -o build/main.bundle.js",
    "build:es6": "webpack --config webpack.front.config.js",
    "build:es6:lux": "webpack --config webpack.front.config.js && cp ./dist/es6-frontend/formkiq-client-sdk-es6.js ../../../lux-it/console/src/assets/dist/formkiq-client-sdk-es6.js",
    "build:cjs": "rollup --config rollup-cjs-for-web.config.js",
    "build:cjs:jobform": "rollup --config rollup-cjs-for-web.config.js && cp ./dist/web-cjs/formkiq-client-sdk-cjs.js ../examples/jobform/formkiq-client-sdk-cjs.js",
    "build:node": "webpack --config webpack.back.config.js"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "amazon-cognito-identity-js": "^4.4.0",
    "buffer": "^5.6.0",
    "node-localstorage": "^2.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-loader": "^8.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
    "webpack-node-externals": "^2.5.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  }
}
